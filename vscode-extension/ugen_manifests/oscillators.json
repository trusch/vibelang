[
  {
    "name": "SinOsc",
    "description": "Interpolating sine wave oscillator. Generates a sine wave using linear interpolation of a wavetable.",
    "rates": ["ar", "kr"],
    "inputs": [
      {"name": "freq", "type": "float", "default": 440.0, "description": "Frequency in Hertz"},
      {"name": "phase", "type": "float", "default": 0.0, "description": "Phase offset or modulator in radians"}
    ],
    "outputs": 1,
    "category": "Oscillators"
  },
  {
    "name": "Saw",
    "description": "Band limited sawtooth wave oscillator. Generates a sawtooth waveform using the SuperCollider sawtooth algorithm.",
    "rates": ["ar", "kr"],
    "inputs": [
      {"name": "freq", "type": "float", "default": 440.0, "description": "Frequency in Hertz"}
    ],
    "outputs": 1,
    "category": "Oscillators"
  },
  {
    "name": "Pulse",
    "description": "Band limited pulse wave oscillator with variable width. Pulse width can be modulated.",
    "rates": ["ar", "kr"],
    "inputs": [
      {"name": "freq", "type": "float", "default": 440.0, "description": "Frequency in Hertz"},
      {"name": "width", "type": "float", "default": 0.5, "description": "Pulse width from 0 to 1. 0.5 is a square wave"}
    ],
    "outputs": 1,
    "category": "Oscillators"
  },
  {
    "name": "LFSaw",
