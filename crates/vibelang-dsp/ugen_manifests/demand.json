[
  {
    "name": "Demand",
    "description": "Demand rate UGen sequencer. Polls demand rate UGens when triggered.",
    "rates": [
      "ar",
      "kr"
    ],
    "inputs": [
      {
        "name": "trig",
        "type": "signal",
        "default": 0,
        "description": "Trigger signal to demand next value"
      },
      {
        "name": "reset",
        "type": "signal",
        "default": 0,
        "description": "Resets the demand rate UGens"
      },
      {
        "name": "demandUGens",
        "type": "signal",
        "default": 0,
        "description": "Demand rate UGens to poll"
      }
    ],
    "outputs": 1,
    "category": "Demand",
    "functions": [
      "demand_ar",
      "demand_kr"
    ]
  },
  {
    "name": "TDuty",
    "description": "Demand rate trigger generator. Generates triggers with inter-onset times from a demand UGen.",
    "rates": [
      "ar",
      "kr"
    ],
    "inputs": [
      {
        "name": "dur",
        "type": "signal",
        "default": 1,
        "description": "Demand UGen providing duration between triggers"
      },
      {
        "name": "reset",
        "type": "signal",
        "default": 0,
        "description": "Reset signal"
      },
      {
        "name": "doneAction",
        "type": "float",
        "default": 0,
        "description": "Action when sequence completes"
      },
      {
        "name": "level",
        "type": "signal",
        "default": 1,
        "description": "Level of output trigger"
      },
      {
        "name": "gapFirst",
        "type": "float",
        "default": 0,
        "description": "If 1, waits first duration before first trigger"
      }
    ],
    "outputs": 1,
    "category": "Demand",
    "functions": [
      "t_duty_ar",
      "t_duty_kr"
    ]
  },
