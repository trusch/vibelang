[
  {
    "name": "PlayBuf",
    "description": "Buffer playback oscillator. Plays back audio from a buffer.",
    "rates": [
      "ar",
      "kr"
    ],
    "inputs": [
      {
        "name": "numChannels",
        "type": "float",
        "default": 1,
        "description": "Number of channels in the buffer"
      },
      {
        "name": "bufnum",
        "type": "float",
        "default": 0,
        "description": "Buffer number to play"
      },
      {
        "name": "rate",
        "type": "float",
        "default": 1,
        "description": "Playback rate (1.0 = normal speed)"
      },
      {
        "name": "trigger",
        "type": "signal",
        "default": 1,
        "description": "Trigger to start playback from beginning"
      },
      {
        "name": "startPos",
        "type": "float",
        "default": 0,
        "description": "Starting frame position in buffer"
      },
      {
        "name": "loop",
        "type": "float",
        "default": 0,
        "description": "Loop mode (0=no loop, 1=loop)"
      },
      {
        "name": "doneAction",
        "type": "float",
        "default": 0,
        "description": "Action when playback finishes"
      }
    ],
    "outputs": 1,
    "category": "Buffers",
    "functions": [
      "play_buf_ar",
      "play_buf_kr"
    ]
  },
