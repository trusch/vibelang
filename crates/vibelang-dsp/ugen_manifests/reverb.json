      {
        "name": "mid",
        "type": "float",
        "default": 1,
        "description": "Mid frequency multiplier"
      },
      {
        "name": "high",
        "type": "float",
        "default": 1,
        "description": "High frequency multiplier"
      },
      {
        "name": "lowcut",
        "type": "float",
        "default": 500,
        "description": "Low/mid crossover frequency"
      },
      {
        "name": "highcut",
        "type": "float",
        "default": 2000,
        "description": "Mid/high crossover frequency"
      }
