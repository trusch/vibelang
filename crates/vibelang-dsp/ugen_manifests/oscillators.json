  {
    "name": "SinOsc",
    "description": "Interpolating sine wave oscillator. Generates a sine wave using linear interpolation of a wavetable.",
    "rates": [
      "ar",
      "kr"
    ],
    "inputs": [
      {
        "name": "freq",
        "type": "float",
        "default": 440,
        "description": "Frequency in Hertz"
      },
      {
        "name": "phase",
        "type": "float",
        "default": 0,
        "description": "Phase offset or modulator in radians"
      }
    ],
    "outputs": 1,
    "category": "Oscillators",
    "functions": [
      "sin_osc_ar",
      "sin_osc_kr"
    ]
  },
