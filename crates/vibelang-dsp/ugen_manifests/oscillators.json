  {
    "name": "Gendy1",
    "description": "Dynamic stochastic synthesis generator. Implements Iannis Xenakis' GENDYN algorithm.",
    "rates": [
      "ar",
      "kr"
    ],
    "inputs": [
      {
        "name": "ampdist",
        "type": "float",
        "default": 1,
        "description": "Amplitude distribution: 0-6 (0=linear, 1=Cauchy, 2=logist, 3=hyperbcos, 4=arcsine, 5=expon, 6=sinus)"
      },
      {
        "name": "durdist",
        "type": "float",
        "default": 1,
        "description": "Duration distribution: 0-6 (same as ampdist)"
      },
      {
        "name": "adparam",
        "type": "float",
        "default": 1,
        "description": "Amplitude distribution parameter"
      },
      {
        "name": "ddparam",
        "type": "float",
        "default": 1,
        "description": "Duration distribution parameter"
      },
      {
        "name": "minfreq",
        "type": "float",
        "default": 440,
        "description": "Minimum frequency in Hertz"
      },
      {
        "name": "maxfreq",
        "type": "float",
        "default": 660,
        "description": "Maximum frequency in Hertz"
      },
      {
        "name": "ampscale",
        "type": "float",
        "default": 0.5,
        "description": "Amplitude scale (0 to 1)"
      },
      {
        "name": "durscale",
        "type": "float",
        "default": 0.5,
        "description": "Duration scale (0 to 1)"
      },
      {
        "name": "initCPs",
        "type": "float",
        "default": 12,
        "description": "Initial number of control points"
      },
      {
        "name": "knum",
        "type": "float",
        "default": 1,
        "description": "Current number of control points"
      }
    ],
    "outputs": 1,
    "category": "Oscillators",
    "functions": [
      "gendy1_ar",
      "gendy1_kr"
    ]
  },
