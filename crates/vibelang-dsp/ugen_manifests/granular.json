[
  {
    "name": "TGrains",
    "description": "Triggered granular synthesis. Generates overlapping grains from a buffer when triggered.",
    "rates": [
      "ar"
    ],
    "inputs": [
      {
        "name": "numChannels",
        "type": "float",
        "default": 2,
        "description": "Number of output channels"
      },
      {
        "name": "trigger",
        "type": "signal",
        "default": 0,
        "description": "Trigger signal to spawn new grain"
      },
      {
        "name": "bufnum",
        "type": "float",
        "default": 0,
        "description": "Buffer number containing audio"
      },
      {
        "name": "rate",
        "type": "float",
        "default": 1,
        "description": "Playback rate (1.0 = normal, 2.0 = double speed)"
      },
      {
        "name": "centerPos",
        "type": "float",
        "default": 0,
        "description": "Grain center position in buffer (0 to 1)"
      },
      {
        "name": "dur",
        "type": "float",
        "default": 0.1,
        "description": "Grain duration in seconds"
      },
      {
        "name": "pan",
        "type": "float",
        "default": 0,
        "description": "Stereo pan position (-1 to 1)"
      },
      {
        "name": "amp",
        "type": "float",
        "default": 0.1,
        "description": "Grain amplitude"
      },
      {
        "name": "interp",
        "type": "float",
        "default": 4,
        "description": "Interpolation type (1=none, 2=linear, 4=cubic)"
      }
    ],
    "outputs": 2,
    "category": "Granular",
    "functions": [
      "t_grains_ar"
    ]
  },
