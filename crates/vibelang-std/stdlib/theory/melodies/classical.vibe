}

// Handel - Hallelujah Chorus - Majestic 4/4
fn hallelujah_chorus() {
    return "A4 - A4 - A4 - E5 - | D5 - C#5 - D5 - - - | A4 - A4 - A4 - E5 - | D5 - C#5 - D5 - - - |";
}

// Mozart - Symphony No. 40 - Fast 4/4
fn mozart_symphony_40() {
    return "Bb4 A4 Bb4 A4 Bb4 C5 A4 - | G4 F4 G4 F4 G4 A4 F4 - |";
}

// Chopin - Nocturne Op. 9 No. 2 - Slow, expressive 12/8
fn nocturne_op9_no2() {
    return "Bb4 - - C5 - - D5 - - Eb5 - - | F5 - - . . . G5 - - Ab5 - - | G5 - - F5 - - Eb5 - - D5 - - | C5 - - Bb4 - - - - - - - - |";
}
